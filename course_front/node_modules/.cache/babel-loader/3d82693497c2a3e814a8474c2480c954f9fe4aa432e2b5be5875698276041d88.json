{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport axios from \"axios\";\nimport store from \"@/store\";\nimport { message } from \"ant-design-vue\";\nexport default {\n  name: \"Login\",\n  setup() {\n    const loginUser = ref({});\n    const onFinish = values => {\n      console.log('Success:', values);\n      axios.post(\"api/login\", {}, {\n        params: {\n          userId: loginUser.value.userId,\n          password: loginUser.value.password\n        }\n      }).then(res => {\n        console.log(res);\n        if (res.data.code === \"200\") {\n          // user.value = res.data.date\n          // 把user对象保存到state\n          store.commit(\"setUser\", res.data.date);\n          message.success(res.data.msg);\n        } else {\n          message.error(res.data.msg);\n        }\n      });\n    };\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n    return {\n      loginUser,\n      onFinish,\n      onFinishFailed\n    };\n  }\n};","map":{"version":3,"mappings":"AAgDA,SAASA,eAAe,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,SAAS;AAC3B,SAAQC,OAAO,QAAO,gBAAgB;AACtC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIN,GAAG,CAAC,CAAC,CAAC,CAAC;IACzB,MAAMO,QAAO,GAAIC,MAAK,IAAK;MACzBC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAEF,MAAM,CAAC;MAC/BP,KAAK,CAACU,IAAI,CAAC,WAAW,EAAC,CAAC,CAAC,EAAC;QACxBC,MAAM,EAAC;UACLC,MAAM,EAAEP,SAAS,CAACQ,KAAK,CAACD,MAAM;UAC9BE,QAAQ,EAAET,SAAS,CAACQ,KAAK,CAACC;QAC5B;MAEF,CAAC,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;QACXR,OAAO,CAACC,GAAG,CAACO,GAAG;QACf,IAAIA,GAAG,CAACC,IAAI,CAACC,IAAG,KAAM,KAAK,EAAC;UAC1B;UACA;UACAjB,KAAK,CAACkB,MAAM,CAAC,SAAS,EAACH,GAAG,CAACC,IAAI,CAACG,IAAI;UACpClB,OAAO,CAACmB,OAAO,CAACL,GAAG,CAACC,IAAI,CAACK,GAAG,CAAC;QAC/B,CAAC,MAAK;UACJpB,OAAO,CAACqB,KAAK,CAACP,GAAG,CAACC,IAAI,CAACK,GAAG,CAAC;QAC7B;MACF,CAAC;IAGH,CAAC;IACD,MAAME,cAAa,GAAIC,SAAQ,IAAK;MAClCjB,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEgB,SAAS,CAAC;IACnC,CAAC;IACD,OAAO;MACLpB,SAAS;MACTC,QAAQ;MACRkB;IACF,CAAC;EACH;AACF","names":["defineComponent","ref","axios","store","message","name","setup","loginUser","onFinish","values","console","log","post","params","userId","value","password","then","res","data","code","commit","date","success","msg","error","onFinishFailed","errorInfo"],"sourceRoot":"","sources":["E:\\WeChat Files\\wxid_77r24v1adq6c22\\FileStorage\\File\\2024-11\\前端代码\\courseweb\\course\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n\r\n  <div class=\"body\">\r\n    <div class=\"container-fluid my-container\" id=\"large-header\">\r\n\r\n      <div class=\"box\">\r\n        <!-- logo -->\r\n        <div class=\"logo\">\r\n\r\n        </div>\r\n        <div class=\"form\">\r\n          <a-form\r\n              :model=\"loginUser\"\r\n              name=\"basic\"\r\n              :label-col=\"{ style: { width: '120px' } }\" :wrapper-col=\"{ span: 14 }\"\r\n              autocomplete=\"off\"\r\n              @finish=\"onFinish\"\r\n              @finishFailed=\"onFinishFailed\"\r\n          >\r\n            <a-form-item\r\n                label=\"登录名\"\r\n                name=\"userId\"\r\n                :rules=\"[{ required: true, message: '请输入登录名！' }]\"\r\n            >\r\n              <a-input v-model:value=\"loginUser.userId\" />\r\n            </a-form-item>\r\n\r\n            <a-form-item\r\n                label=\"密码\"\r\n                name=\"password\"\r\n                :rules=\"[{ required: true, message: '请输入密码！' }]\"\r\n            >\r\n              <a-input-password v-model:value=\"loginUser.password\"  />\r\n            </a-form-item>\r\n\r\n            <a-form-item :wrapper-col=\"{ offset: 10, span: 15 }\">\r\n              <a-button type=\"primary\" html-type=\"submit\">登录</a-button>\r\n            </a-form-item>\r\n          </a-form>\r\n        </div>\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, ref } from 'vue';\r\nimport axios from \"axios\";\r\nimport store from \"@/store\";\r\nimport {message} from \"ant-design-vue\";\r\nexport default {\r\n  name: \"Login\",\r\n  setup() {\r\n    const loginUser = ref({});\r\n    const onFinish = values => {\r\n      console.log('Success:', values);\r\n      axios.post(\"api/login\",{},{\r\n        params:{\r\n          userId: loginUser.value.userId,\r\n          password: loginUser.value.password\r\n        }\r\n\r\n      }).then(res=>{\r\n        console.log(res)\r\n        if (res.data.code === \"200\"){\r\n          // user.value = res.data.date\r\n          // 把user对象保存到state\r\n          store.commit(\"setUser\",res.data.date)\r\n          message.success(res.data.msg);\r\n        }else {\r\n          message.error(res.data.msg);\r\n        }\r\n      })\r\n\r\n\r\n    };\r\n    const onFinishFailed = errorInfo => {\r\n      console.log('Failed:', errorInfo);\r\n    };\r\n    return {\r\n      loginUser,\r\n      onFinish,\r\n      onFinishFailed,\r\n    };\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.body {\r\n  width: 100%;\r\n  height: 100vh;\r\n}\r\n\r\n.my-container {\r\n  height: 100vh;\r\n  background-image: url(\"../../public/img/bg.jpeg\");\r\n  background-repeat: no-repeat;\r\n  background-size: 100% 100%;\r\n}\r\n\r\n.box {\r\n  position: absolute;\r\n  left: 420px;\r\n  width: 460px;\r\n  height: 380px;\r\n  background-color: #ffffffaa;\r\n  margin: 180px auto;\r\n  border-radius: 5px;\r\n  z-index: 999;\r\n}\r\n\r\n.logo {\r\n  width: 100%;\r\n  height: 80px;\r\n  padding: 10px 5px 5px;\r\n  background-image: url(\"../../public/img/logo.png\");\r\n  background-size: 100% 100%;\r\n}\r\n.form {\r\n  width: 100%;\r\n  height: calc(440px - 80px);\r\n  padding: 55px 56px 10px 10px;\r\n  margin-left: 23px;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}